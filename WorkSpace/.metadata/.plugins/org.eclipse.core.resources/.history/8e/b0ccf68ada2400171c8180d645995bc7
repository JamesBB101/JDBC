package com.iii.BB101.james;

import java.sql.Date;

public class test {

	public static void main(String[] args) {
		java.util.Date du = new java.util.Date();
		long long1 = du.getTime();
		Date sqlDate = new Date(long1);
		System.out.println(sqlDate);

		java.util.Date du2 = new java.util.Date();
		long long2 = du2.getTime();
		java.sql.Date ds2 = new java.sql.Date(long1);
		String now2 = ds2.toString();
		System.out.println(now2);
	}

}
